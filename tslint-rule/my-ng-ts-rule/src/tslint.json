{
  "extends": "../tslint.json",
  "rulesDirectory": [
    "../tsrule-src"
  ],
  "rules": {
    "no-self": true,
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "app",
      "kebab-case"
    ]
  }
}
